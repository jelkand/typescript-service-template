{
  "presets": ["@babel/env", "@babel/typescript"],
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread",
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "test": "./test"
        },
        "extensions": [".ts", ".tsx", ".js", ".jsx", ".es", ".es6", ".mjs"]
      }
    ]
  ]
}
