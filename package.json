{
  "name": "typescript-service-template",
  "version": "0.1.0",
  "description": "An opinionated template for spinning up typescript services.",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist --extensions \".ts\" --source-maps inline",
    "build:js:watch": "babel src -w --out-dir dist --extensions \".ts\" --source-maps inline",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint --fix .",
    "start:ci": "node dist/server/server.js",
    "start:dev": "concurrently \"npm run build:js:watch\" \"nodemon dist/server/server.js\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jelkand/typescript-service-template.git"
  },
  "author": "Jack Anderson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jelkand/typescript-service-template/issues"
  },
  "homepage": "https://github.com/jelkand/typescript-service-template#readme",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^24.0.17",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "concurrently": "^4.1.1",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@types/bluebird": "^3.5.27",
    "@types/express": "^4.17.0",
    "@types/node": "^12.7.1",
    "@types/sequelize": "^4.28.4",
    "@types/validator": "^10.11.2",
    "express": "^4.17.1",
    "sequelize": "^5.14.0",
    "sequelize-cli": "^5.5.0"
  }
}
